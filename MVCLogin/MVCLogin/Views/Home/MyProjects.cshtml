@{
    ViewBag.Title = "MyProjects";
}


<!-- PROJEKTY-->
<div class="col-lg-1">
    <h1 class="vertical-text" style="text-align: center;margin-left: 5%; margin-right: 4%; margin-top: 350px" id="lefttitle">
        @MVCLogin.Resources.HomeTexts.PROJECTS
    </h1>
</div>

<div class="container" style="margin-left: auto; margin-top: 50px">
    <div class="row" style="margin-left:50px">
       

            <!-- FIRST IMG IN FIRST ROW -->

            <div class="col-lg-4">
                <a href="~/Content/projects/euromax/index.html" target="_blank">
                    <img src="~/Content/img/projects/euromax.jpg"
                         class="img-fluid proj-img-size rounded mx-auto d-block" alt="Responsive image" title="FIRMA DEWELOPERSKA EUROMAX">
                </a>
                <div class="col-lg-0">
                    <Button class="btn btn-default btn-sm btn-menu-space float-right" Style="height: 30px; margin-top: 8px"
                            data-toggle="collapse" href="#euromax" role="button" aria-expanded="false" aria-controls="collapseExample">
                        @MVCLogin.Resources.HomeTexts.MOREBUTTON
                    </Button>
                </div>
                <div class="row" style="margin-left:10px; margin-top: 10px">
                    <div class="col-lg-0">
                        <img src="~/Content/img/htmlicon.png" class="small-ico-size d-block img-fluid" title="HTML" />
                    </div>
                    <div class="col-lg-0">
                        <img src="~/Content/img/cssicon.png" class="small-ico-size d-block img-fluid" title="CSS3" />
                    </div>
                </div>
                <div class="collapse" id="euromax">
                    <div class="card card-body" style="margin-top:20px; max-width:300px">
                        <p class="text-justify">
                            @MVCLogin.Resources.HomeTexts.EUROMAXTEXT

                        </p>

                    </div>
                </div>
            </div>

            <!-- SECOND IMG IN FIRST ROW -->

            <div class="col-lg-4">
                <a href="~/Content/projects/szlips/index.html" target="_blank">
                    <img src="~/Content/img/projects/szlips.jpg"
                         class="img-fluid proj-img-size rounded mx-auto d-block" alt="Responsive image" title="KANCELARIA BROKERSKA SZLIPS">
                </a>
                <div class="col-lg-0">
                    <Button class="btn btn-default btn-sm btn-menu-space float-right" Style="height: 30px; margin-top: 8px"
                            data-toggle="collapse" href="#szlips" role="button" aria-expanded="false" aria-controls="collapseExample">
                        @MVCLogin.Resources.HomeTexts.MOREBUTTON
                    </Button>
                </div>
                <div class="row" style="margin-left:10px; margin-top: 10px">
                    <div class="col-lg-0">
                        <img src="~/Content/img/htmlicon.png" class="small-ico-size d-block img-fluid" title="HTML" />
                    </div>
                    <div class="col-lg-0">
                        <img src="~/Content/img/cssicon.png" class="small-ico-size d-block img-fluid" title="CSS3" />
                    </div>
                    <div class="col-lg-0">
                        <img src="~/Content/img/bootstrapicon.png" class="small-ico-size d-block img-fluid" title="BOOTSTRAP" />
                    </div>
                </div>
                <div class="collapse" id="szlips">
                    <div class="card card-body" style="margin-top:20px; max-width:300px">
                        <p class="text-justify">
                            @MVCLogin.Resources.HomeTexts.SZLIPSTEXT

                        </p>
                    </div>
                </div>
            </div>

            <!-- THIRD IMG IN FIRST ROW -->
            <div class="col-lg-4">


                <a href="~/Content/projects/insurance/InsuranceApp.exe" target="_blank">
                    <img src="~/Content/img/projects/insuranceapp.jpg"
                         class="img-fluid proj-img-size rounded mx-auto d-block" alt="Responsive image" title="KONFIGURATOR OFERT">
                </a>
                <div class="col-lg-0">
                    <Button class="btn btn-default btn-sm btn-menu-space float-right" Style="height: 30px; margin-top: 8px"
                            data-toggle="collapse" href="#insurance" role="button" aria-expanded="false" aria-controls="collapseExample">
                        @MVCLogin.Resources.HomeTexts.MOREBUTTON
                    </Button>
                </div>
                <div class="row" style="margin-left:10px; margin-top: 10px">
                    <div class="col-lg-0">
                        <img src="~/Content/img/csharpicon.png" class="small-ico-size d-block img-fluid" title="C SHARP" />
                    </div>
                    <div class="col-lg-0">
                        <img src="~/Content/img/wpficon.png" class="small-ico-size d-block img-fluid" title=".NET WPF" />
                    </div>
                    <div class="col-lg-0">
                        <img src="~/Content/img/visualicon.png" class="small-ico-size d-block img-fluid" title="VISUAL STUDIO 2017" />
                    </div>
                </div>
                <div class="collapse" id="insurance">
                    <div class="card card-body" style="margin-top:20px; max-width:300px">
                        <p class="text-justify">
                            @MVCLogin.Resources.HomeTexts.APPTEXT

                        </p>
                    </div>
                </div>

            </div>

        </div>
    </div>



